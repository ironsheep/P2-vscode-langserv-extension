{
	"arrowParens": "always",
	"bracketSameLine": true,
	"bracketSpacing": true,
	"endOfLine": "auto",
	"experimentalTernaries": false,
	"insertPragma": false,
	"semi": true,
	"singleAttributePerLine": false,
	"singleQuote": true,
	"tabWidth": 2,
	"trailingComma": "none",
	"overrides": [
		{
			"files": ["package.json", "scripts/*.json"],
			"options": {
				"parser": "json"
			}
		}
	]
  }
